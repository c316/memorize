{
  "expo": {
    "name": "CPLS Memorize",
    "githubUrl": "https://github.com/c316/memorize",
    "orientation": "portrait",
    "splash": {
      "resizeMode": "cover",
      "image": "./assets/images/splashScreen.png"
    },
    "version": "2.4.6",
    "slug": "cpls-memorize",
    "sdkVersion": "28.0.0",
    "ios": {
      "bundleIdentifier": "com.joshjoe.memorize",
      "icon": "./assets/images/ios-icon.png",
      "supportsTablet": true
    },
    "android": {
      "package": "com.joshjoe.memorize",
      "versionCode": 102405,
      "permissions": ["com.android.launcher.permission.INSTALL_SHORTCUT"],
      "icon": "./assets/images/CPLS-Lion-logo-512x512.png"
    },
    "loading": {
      "icon": "./assets/images/CPLS-Lion-logo-512x512.png"
    },
    "packagerOpts": {
      "assetExts": ["ttf"]
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD"
    }
  }
